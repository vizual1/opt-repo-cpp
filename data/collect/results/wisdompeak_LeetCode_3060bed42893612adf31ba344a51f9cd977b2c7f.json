      "--- Greedy/2171.Removing-Minimum-Number-of-Magic-Beans/2171.Removing-Minimum-Number-of-Magic-Beans.cpp\n@@ -4,25 +4,12 @@ class Solution {\n     long long minimumRemoval(vector<int>& beans) \n     {\n         sort(beans.rbegin(), beans.rend());\n-        int n = beans.size();\n-        \n-        vector<LL>pre(n);\n-        pre[0] = beans[0];\n-        for (int i=1; i<n; i++)\n-            pre[i] = pre[i-1]+(LL)beans[i];        \n-        \n-        vector<LL>suf(n);\n-        suf[n-1] = beans[n-1];\n-        for (int i=n-2; i>=0; i--)\n-            suf[i] = suf[i+1]+(LL)beans[i];\n-                \n+        LL total = accumulate(beans.begin(), beans.end(), 0LL);\n+\n         LL ret = LLONG_MAX;\n-        for (int i=0; i<n; i++)\n-        {\n-            LL ans = 0;\n-            ans += pre[i] - (LL)(i+1)*beans[i];\n-            ans += (i+1==n)?0:suf[i+1];\n-            ret = min(ret, ans);\n+        for (int i=0; i<beans.size(); i++)\n+        {            \n+            ret = min(ret, total - (LL)beans[i]*(i+1));\n         }\n         return ret;        \n     }"
    ],
